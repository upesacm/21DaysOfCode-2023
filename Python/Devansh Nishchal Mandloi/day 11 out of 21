#Que1}

p=int(input("Enter the divident:"))
q=int(input("Enter the divisor:"))

try:
    result = p/q
except ZeroDivisionError:
    print("The divisor can never be zero, please re-enter the numbers")
else:
    print(f"The result of the division of p by q is {result}")


#Que2}

file_name=input("Enter the name of a file:")

try:
    file=open(file_name, "r")
except FileNotFoundError:
    print("The file you are trying to access does not exist")
else:
    print("Yayy!")
