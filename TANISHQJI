#include<stdio.h>
int main() {
    int number1;
    printf("enter your number1");
    scanf("%d",&number1);
    int number2;
    printf("enter your number2");
    scanf("%d",&number2);
    int number3;
    printf("enter you number3");
    scanf("%d",&number3);
    int add;
    add=number1+number2+number3;
    scanf("12%d",&add);
    return 0;


}


